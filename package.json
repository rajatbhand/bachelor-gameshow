{
  "name": "bachelor-gameshow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_PUBLIC_FIREBASE_ENV=testing next dev",
    "dev:test": "NEXT_PUBLIC_FIREBASE_ENV=testing next dev",
    "dev:prod": "NEXT_PUBLIC_FIREBASE_ENV=production next dev",
    "build": "next build",
    "build:test": "NEXT_PUBLIC_FIREBASE_ENV=testing next build",
    "build:prod": "NEXT_PUBLIC_FIREBASE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build",
    "export:test": "NEXT_PUBLIC_FIREBASE_ENV=testing next build",
    "export:prod": "NEXT_PUBLIC_FIREBASE_ENV=production next build",
    "deploy": "npm run export:test && firebase deploy -P testing",
    "deploy:test": "npm run export:test && firebase deploy -P testing --only hosting",
    "deploy:prod": "npm run export:prod && firebase deploy -P production --only hosting"
  },
  "dependencies": {
    "firebase": "^10.7.1",
    "next": "14.2.31",
    "papaparse": "^5.5.3",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.31",
    "firebase-tools": "^13.4.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}